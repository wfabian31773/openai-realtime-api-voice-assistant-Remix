APP_ENV=production
DOMAIN=your-app-domain.replit.app

# CRITICAL: Use Supabase Transaction Pooler URL (port 6543) for high-volume production
# This is REQUIRED for handling 20+ concurrent calls without connection exhaustion
# Port 6543 = Transaction pooling (recommended for voice agents)
# Get this from Supabase Dashboard > Settings > Database > Connection Pooling > Transaction
DATABASE_URL=postgres://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_ID.supabase.co:6543/postgres

# OPTIONAL: Direct connection URL for Drizzle migrations (if needed)
# SUPABASE_URL=postgres://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_ID.supabase.co:5432/postgres
SUPABASE_REST_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_SERVICE_KEY=your-supabase-service-key

# OpenAI Configuration
# Get these from https://platform.openai.com/
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_PROJECT_ID=proj_your-project-id
OPENAI_WEBHOOK_SECRET=whsec_your-webhook-secret

# Twilio Configuration
# Get these from https://console.twilio.com/
TWILIO_ACCOUNT_SID=ACyour-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1XXXXXXXXXX
HUMAN_AGENT_NUMBER=+1XXXXXXXXXX
URGENT_NOTIFICATION_NUMBER=+1XXXXXXXXXX

# Ticketing System Integration
TICKETING_API_KEY=your-ticketing-api-key
TICKETING_SYSTEM_URL=https://your-ticketing-system.example.com
VOICE_AGENT_WEBHOOK_SECRET=your-webhook-secret

# Session Security
# Generate with: openssl rand -base64 64
SESSION_SECRET=your-session-secret-generate-with-openssl

# PHI/PII Logging Control
# Set to true in production to disable sensitive data logging
DISABLE_PHI_LOGGING=true
